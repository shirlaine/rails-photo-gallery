<%= form_for @photo, :url=> photos_path do |f| %>
	<%= f.label :title, "Title" %>
	<%= f.text_field :title %>

	<%= f.label :date, "Date" %>
	<%= f.text_field :date %>

	<%= f.label :description, "Description" %>
	<%= f.text_field :description %>

	<%= f.label :file_location, "File_location" %>
	<%= f.text_field :file_location %>

	<%= f.submit "Create" %>
	<% end %> 